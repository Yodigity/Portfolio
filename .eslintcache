 } = event.target;\n    setState({ ...state, [name]: value });\n  };\n\n  const prevStep = () => {\n    if (state.currentStep > 1 && state.currentStep <= 3) {\n      setState({ ...state, currentStep: state.currentStep - 1 });\n      console.log(state);\n    }\n  };\n\n  const nextStep = () => {\n    return state.currentStep < 3\n      ? setState({ ...state, currentStep: state.currentStep + 1 })\n      : null;\n  };\n\n  const handleSubmit = () => {\n    console.log(state);\n  };\n\n  return (\n    <div className='contactForm'>\n      <form onSubmit={() => handleSubmit()} style={{ margin: \"2em 0 0 0\" }}>\n        <StepOne\n          currentStep={state.currentStep}\n          name={state.name}\n          handleChange={handleChange}\n        />\n        <StepTwo\n          currentStep={state.currentStep}\n          email={state.email}\n          handleChange={handleChange}\n        />\n        <StepThree\n          currentStep={state.currentStep}\n          message={state.message}\n          handleChange={handleChange}\n        />\n      </form>\n\n      <div className='step-nav-buttons'>\n        {state.currentStep > 1 && state.currentStep <= 3 ? (\n          <button className='nav-step' onClick={() => prevStep()}>\n            Prev\n          </button>\n        ) : (\n          \"\"\n        )}\n\n        {state.currentStep < 3 ? (\n          <button className='nav-step' onClick={() => nextStep()}>\n            Next\n          </button>\n        ) : (\n          \"\"\n        )}\n        {state.currentStep == 3 ? (\n          <button\n            className='nav-step'\n            type='submit'\n            onSubmit={() => handleSubmit()}\n          >\n            Send\n          </button>\n        ) : (\n          \"\"\n        )}\n      </div>\n    </div>\n  );\n};\n","/Users/aarongokani/Desktop/React/portfolio/portfolio/src/Components/Contact/StepOne.js",[],"/Users/aarongokani/Desktop/React/portfolio/portfolio/src/Components/Contact/StepTwo.js",[],"/Users/aarongo